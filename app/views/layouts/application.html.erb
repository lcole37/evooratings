<!DOCTYPE html>
<html>
  <head>
    <title>Evoo</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>

    <nav>
  <%= link_to "Home", root_path %> <%= link_to "Log In", "/login" unless session[:user_id]%>
  </nav>
  <% flash.each do |name, msg| %>
    <%= content_tag :div, msg, class: name %>
  <% end %>
  
    <%= yield %>
  </body>
</html>
